/* pages/book_lists/book_lists.wxss */
.wrap{
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 20rpx;
}
navigator{
  overflow: hidden;
}
.list{
  margin-bottom: 20rpx;
  height: 200rpx;
  position: relative;
}
.imgs{
  float: left;
}
.imgs image{
  display: block;
  width: 200rpx;
  height: 200rpx;
}
.infos{
  float: left;
  width: 400rpx;
  height: 200rpx;
  padding: 20rpx 0 0 20rpx;
}
.title{
  font-size: 40rpx;
}
.date{
  font-size: 25rpx;
  color: #aaa;
  position: absolute;
  bottom: 0;
}